
survey deep -title "Blind bedding plane at bottom of God series"

input deep-e.th2

map m-all-e -projection extended
    deep-1e
endmap

input deep-p.th2

map m-all-p -projection plan
    deep-1p
    deep-2p
    deep-3p
    deep-4p
endmap

join deep-4p deep-3p
join deep-3p deep-2p
join deep-2p deep-1p

centreline
    team "James Hooper" insts notes
    explo-team "James Hooper"
    date 1997.08.03
    units length meters
    units compass clino degrees
    data normal from to tape compass clino ignoreall # Default
    1	2	9.55	261	-59	#1 is bolt near carbide S on roof
    2	3	11.00	-	down	#2 is bolt
    3	4	8.80	287	-56
    4	5	4.12	275	-67	#4 is bolt
    5	6	2.79	320	-33
    6	7	10.96	319	-63	#6 is bolt	
    7	8	5.77	285	-40
    8	9	20.60	265	-67
    9	10	4.38	199	-34
    10	11	15.62	259	-60	#10 is bolt
    11	12	12.50	352	-24
endcentreline
endsurvey

