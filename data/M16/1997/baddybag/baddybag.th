
survey baddybag -title "System Migovec"
	
	input "baddybag-e.th2"
	
	map m-all-e -projection extended
		baddybag-1e
	endmap

	input "baddybag-p.th2"
	
	map m-all-p -projection plan
		baddybag-3p
		break
		baddybag-2p
		baddybag-1p
	endmap
	
	centreline
		team "Alva Gosson" insts notes
		team "Clewin Griffith" insts notes
		team "Tim Wright" insts notes
		
		explo-team "Alva Gosson"
		explo-team "Tim Wright"
		explo-team "Clewin Griffith"
		explo-date 1997.08.02
		date 1997.08.02
		units length meters
		units compass clino degrees
		data normal from to tape compass clino
		1	2	2.90	17	5	#1 PSS on cairn
		2	3	3.67	-	up
		3	4	2.20	20	13
		4	5	14.47	-	up
		5	6	6	240	-40	#Leg to aven distance guessed
		5	7	7.76	100	55
		7	8	3.65	39	35	#SS7 is prayer bell
		8	9	3.50	27	-5
		9	10	14.23	-	up
		9	11	8	135	35	#Another aven, distance guessed
		10	12	2.12	56	-25
		13	12	7.99	15	2
		14	13	1.55	79	-1
	endcentreline
endsurvey

